<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>
    <style name="Flo" parent="Widget.Design.FloatingActionButton">
<item name="shapeAppearanceOverlay">@style/button2</item>



    </style>
    <style name="Flo1" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/button</item>



    </style>
    <style name="button" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">50%</item>

        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">cut</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">cut</item>

    </style>
    <style name="button2" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>

    </style>

</resources>